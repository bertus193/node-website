<div>
   <ul class="list-group">  
      <% images.forEach( function( image ) { %>
           <li id="<%- image.enlace %>" class="list-group-item PaginationItem"><img class="PaginationImg" src="/images/lib/<%- image.pathName %>"></img>
           <button class="PaginationButtons btn btn-primary" onClick="deleteImage('/images/delete/<%- image.enlace %>' ,'<%- image.enlace %>', '<%- currentPage %>' )">
             <i class="fa fa-trash" aria-hidden="true"></i></button>

           <a href="/images/edit/<%= image.enlace %>"><div class="PaginationButtons btn btn-primary buttonEdit">
               <i class="fa fa-pencil-square-o" aria-hidden="true"></i>
           </div></a>
           <a href="/images/<%= image.enlace %>"><div class="PaginationButtons btn btn-primary buttonDelete">
               <i class="fa fa-arrow-right" aria-hidden="true"></i>
           </div></a>
          </li>
      <% }) %>
   </ul>
</div>



<% if (pageCount > 1) { %>  
   <ul class="pagination">  
   <% var i = 1;
   if (currentPage > 5) {
      i = +currentPage - 4;
   } %>
   <% if (i !== 1) { %>
      <li class="disabled"><a href="#">...</a></li>
   <% } %>
   <% for (i; i<=pageCount; i++) { %>
      <% if (currentPage == i) { %>
            <li class="active"><span><%= i %></span></li>
      <% }else if (i > (+currentPage + 4)) { %>
            <li class="disabled"><a href="#">...</a></li>
      <%    break;
         } else { %>
            <li><a style="cursor: pointer;" onClick="cargarGaleria('/functions/verGaleria?page=<%= i %>')"><%= i %></a></li>
      <% } %>
   <% } %>
   </ul>  
<% } %> 